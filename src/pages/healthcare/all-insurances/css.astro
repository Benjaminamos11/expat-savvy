---
import Layout from '../../../layouts/Layout.astro';
import ConsultationModal from '../../../components/ConsultationModal.astro';
import ExpandableFormBanner from '../../../components/ExpandableFormBanner.astro';

const tableOfContents = [
  { id: "overview", title: "Overview & Key Features" },
  { id: "premiums", title: "Premium Structure" },
  { id: "basic-insurance", title: "Basic Insurance Models" },
  { id: "supplementary", title: "Supplementary Insurance" },
  { id: "digital", title: "Digital Services" },
  { id: "expats", title: "For Expats" },
  { id: "apply", title: "How to Apply" },
  { id: "faqs", title: "FAQs" }
];

const features = [
  {
    title: "Basic Insurance Models",
    description: "Four flexible models including standard, family doctor, HMO/GVP, and Telmed options",
    icon: `<svg class="h-6 w-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" />
    </svg>`
  },
  {
    title: "myFlex Insurance Solutions",
    description: "Comprehensive supplementary coverage with outpatient, hospitalization, and alternative medicine options",
    icon: `<svg class="h-6 w-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19.428 15.428a2 2 0 00-1.022-.547l-2.387-.477a6 6 0 00-3.86.517l-.318.158a6 6 0 01-3.86.517L6.05 15.21a2 2 0 00-1.806.547M8 4h8l-1 1v5.172a2 2 0 00.586 1.414l5 5c1.26 1.26.367 3.414-1.415 3.414H4.828c-1.782 0-2.674-2.154-1.414-3.414l5-5A2 2 0 009 10.172V5L8 4z" />
    </svg>`
  },
  {
    title: "Digital Services",
    description: "Modern mobile app for policy management and invoice scanning",
    icon: `<svg class="h-6 w-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 18h.01M8 21h8a2 2 0 002-2V5a2 2 0 00-2-2H8a2 2 0 00-2 2v14a2 2 0 002 2z" />
    </svg>`
  }
];

const supplementaryPlans = [
  {
    name: "myFlex Outpatient",
    description: "Worldwide coverage for outpatient treatments",
    benefits: [
      "Eye laser surgery coverage",
      "Dental insurance for children",
      "Global coverage",
      "Alternative medicine"
    ],
    highlight: true
  },
  {
    name: "myFlex Hospitalisation",
    description: "Comprehensive hospital coverage across Switzerland",
    benefits: [
      "Free hospital choice",
      "Private/semi-private ward",
      "Nationwide coverage",
      "Emergency abroad"
    ]
  },
  {
    name: "myFlex Alternative",
    description: "Coverage for alternative medicine treatments",
    benefits: [
      "Approved therapists",
      "Various methods covered",
      "Preventive care",
      "Holistic approach"
    ]
  },
  {
    name: "Health Account",
    description: "Preventative health benefits",
    benefits: [
      "Sports club discounts",
      "Yoga classes",
      "Fitness programs",
      "Wellness activities"
    ]
  }
];

const premiumComparison = {
  ageGroups: [
    {
      group: "Adults (26+)",
      css: "From CHF 310",
      marketAvg: "CHF 320",
      savings: "Up to 3%"
    },
    {
      group: "Young Adults (19-25)",
      css: "From CHF 230",
      marketAvg: "CHF 240",
      savings: "Up to 4%"
    },
    {
      group: "Children (0-18)",
      css: "From CHF 95",
      marketAvg: "CHF 100",
      savings: "Up to 5%"
    }
  ]
};

const faqs = [
  {
    question: "What makes CSS different from other Swiss insurers?",
    answer: "As Switzerland's largest health insurer, CSS offers extensive coverage options, a comprehensive network of service points, and innovative digital solutions. Their myFlex insurance solutions provide flexible coverage that can be tailored to individual needs."
  },
  {
    question: "How do I get a quote from CSS?",
    answer: "You can get a personalized quote from CSS through their online premium calculator, by scheduling a free consultation with our experts, or visiting a local CSS office. We recommend comparing multiple offers to find the best coverage for your needs."
  },
  {
    question: "What digital services does CSS offer?",
    answer: "CSS provides a modern mobile app for policy management, digital insurance cards, online claims submission, and invoice scanning capabilities. Their digital platform makes insurance management simple and efficient."
  },
  {
    question: "Does CSS offer English-speaking customer service?",
    answer: "Yes, CSS provides multilingual customer service, including English support, through their customer service centers and digital platforms."
  },
  {
    question: "How can I submit claims to CSS?",
    answer: "CSS offers multiple ways to submit claims: through their mobile app, online portal, or by mail. The digital options allow for quick submission by simply photographing your medical bills and uploading them directly."
  }
];

---

<Layout
  title="CSS Health Insurance Switzerland | 2025 Expert Guide for Expats"
  description="Compare CSS health insurance plans in Switzerland. Learn about their innovative digital services, comprehensive coverage options, and exclusive benefits. Updated for 2025."
>
  <!-- Schema.org markup -->
  <script type="application/ld+json" slot="head">
    {
      "@context": "https://schema.org",
      "@type": "InsuranceProduct",
      "name": "CSS Health Insurance Switzerland",
      "description": "Comprehensive health insurance coverage from CSS, Switzerland's largest health insurance provider",
      "url": "https://expat-savvy.ch/healthcare/all-insurances/css",
      "logo": "https://res.cloudinary.com/dphbnwjtx/image/upload/v1740414664/css-logo_yvxvxr.png",
      "provider": {
        "@type": "Organization",
        "name": "CSS",
        "address": {
          "@type": "PostalAddress",
          "streetAddress": "Tribschenstrasse 21",
          "addressLocality": "Luzern",
          "postalCode": "6005",
          "addressCountry": "CH"
        }
      },
      "areaServed": "CH",
      "hasOfferCatalog": {
        "@type": "OfferCatalog",
        "name": "Insurance Products",
        "itemListElement": [
          {
            "@type": "Offer",
            "itemOffered": {
              "@type": "Service",
              "name": "Basic Insurance",
              "description": "Multiple models including standard, family doctor, HMO/GVP, and Telmed options"
            }
          },
          {
            "@type": "Offer",
            "itemOffered": {
              "@type": "Service",
              "name": "myFlex Insurance",
              "description": "Comprehensive supplementary coverage with outpatient, hospitalization, and alternative medicine options"
            }
          }
        ]
      }
    }
  </script>

  <!-- Last Updated Banner -->
  <div class="bg-primary-50 py-2">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <p class="text-sm text-primary-700 text-center">
        Last Updated: January 15, 2025
      </p>
    </div>
  </div>

  <!-- Table of Contents -->
  <div class="bg-white sticky top-0 z-10 border-b shadow-sm">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <nav class="flex items-center space-x-4 overflow-x-auto py-4">
        {tableOfContents.map(item => (
          <a 
            href={`#${item.id}`}
            class="text-sm text-gray-600 hover:text-primary-600 whitespace-nowrap"
          >
            {item.title}
          </a>
        ))}
      </nav>
    </div>
  </div>

  <!-- About Section with Highlighted Expat Advantages -->
  <div class="bg-white py-16">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="prose prose-lg mx-auto">
        <h2 class="text-3xl font-bold text-gray-900 text-center mb-8">About CSS</h2>
        <p>
          The origins of CSS date back to 1899 when it was founded as the Christian Social Health Insurance Fund of Switzerland. Headquartered in Lucerne, CSS insures roughly 1.34 million policy holders under the compulsory basic insurance model. A mobile app provides fingertip access to policy information, and invoices can be scanned using the smartphone camera.
        </p>
        <p>
          Encompassing all insurance offerings, CSS insures some 1.7 million persons with annual revenues of 6.17 billion Swiss francs. The CSS Group includes additional health insurance providers: Arcosana, Intras and Sanagate.
        </p>
        
        <!-- Highlighted Expat Advantages -->
        <div class="bg-white rounded-xl shadow-lg p-6 my-8">
          <h3 class="text-xl font-semibold text-gray-900 mb-4">Expat Advantages with CSS</h3>
          <div class="grid grid-cols-1 md:grid-cols-2 gap-4">
            <div class="flex items-start">
              <svg class="h-5 w-5 text-primary-500 mt-1 mr-3 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
              </svg>
              <div>
                <span class="font-medium">Multilingual Support</span>
                <p class="text-sm text-gray-600">Customer service in multiple languages</p>
              </div>
            </div>
            <div class="flex items-start">
              <svg class="h-5 w-5 text-primary-500 mt-1 mr-3 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
              </svg>
              <div>
                <span class="font-medium">Digital Services</span>
                <p class="text-sm text-gray-600">Modern app and online platform</p>
              </div>
            </div>
            <div class="flex items-start">
              <svg class="h-5 w-5 text-primary-500 mt-1 mr-3 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
              </svg>
              <div>
                <span class="font-medium">Nationwide Network</span>
                <p class="text-sm text-gray-600">Extensive service points</p>
              </div>
            </div>
            <div class="flex items-start">
              <svg class="h-5 w-5 text-primary-500 mt-1 mr-3 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
              </svg>
              <div>
                <span class="font-medium">Comprehensive Coverage</span>
                <p class="text-sm text-gray-600">Wide range of insurance options</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Expandable Form Banner -->
  <div class="bg-white py-16">
    <ExpandableFormBanner provider="CSS" />
  </div>

  <!-- Features Section -->
  <div class="bg-gray-50 py-16">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="grid grid-cols-1 gap-8 md:grid-cols-3">
        {features.map(feature => (
          <div class="bg-white rounded-lg shadow-md p-6 transform transition-transform hover:scale-105">
            <div class="text-primary-500 mb-4">
              <Fragment set:html={feature.icon} />
            </div>
            <h3 class="text-lg font-semibold text-gray-900 mb-2">{feature.title}</h3>
            <p class="text-gray-600">{feature.description}</p>
          </div>
        ))}
      </div>
    </div>
  </div>

  <!-- Premium Structure -->
  <div id="premiums" class="bg-white py-16">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="max-w-3xl mx-auto">
        <h2 class="text-3xl font-bold text-gray-900 mb-8">Premium Structure & Cost Advantages</h2>
        
        <!-- Premium Comparison Table -->
        <div class="bg-white rounded-lg shadow-md overflow-hidden mb-8">
          <table class="min-w-full divide-y divide-gray-200">
            <thead class="bg-gray-50">
              <tr>
                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Age Group</th>
                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">CSS Premium</th>
                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Market Average</th>
                <th scope="col" class="px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider">Potential Savings</th>
              </tr>
            </thead>
            <tbody class="bg-white divide-y divide-gray-200">
              {premiumComparison.ageGroups.map(group => (
                <tr>
                  <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">{group.group}</td>
                  <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">{group.css}</td>
                  <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">{group.marketAvg}</td>
                  <td class="px-6 py-4 whitespace-nowrap text-sm text-primary-600 font-medium">{group.savings}</td>
                </tr>
              ))}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

  <!-- Supplementary Plans Section -->
  <div class="bg-gray-50 py-16">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8" id="supplementary">
      <h2 class="text-3xl font-bold text-gray-900 mb-12 text-center">Supplementary Insurance Plans</h2>
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8">
        {supplementaryPlans.map(plan => (
          <div class={`bg-white rounded-lg shadow-md p-6 border ${plan.highlight ? 'border-primary-500' : 'border-gray-200'} hover:border-primary-500 transition-colors relative`}>
            {plan.highlight && (
              <div class="absolute -top-4 -right-4 bg-primary-500 text-white px-3 py-1 rounded-full text-sm font-medium shadow-lg">
                Popular Choice
              </div>
            )}
            <h3 class="text-xl font-semibold text-gray-900 mb-3">{plan.name}</h3>
            <p class="text-gray-600 mb-4">{plan.description}</p>
            <ul class="space-y-2">
              {plan.benefits.map(benefit => (
                <li class="flex items-center text-sm text-gray-500">
                  <svg class="h-4 w-4 text-primary-500 mr-2" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"/>
                  </svg>
                  {benefit}
                </li>
              ))}
            </ul>
          </div>
        ))}
      </div>
    </div>
  </div>

  <!-- Digital Services -->
  <div id="digital" class="bg-white py-16">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="max-w-3xl mx-auto">
        <h2 class="text-3xl font-bold text-gray-900 mb-8">Digital Services & Customer Experience</h2>
        <div class="bg-white rounded-lg shadow-md p-6 border border-gray-200">
          <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
            <div>
              <h3 class="text-xl font-semibold text-gray-900 mb-4">Mobile App Features</h3>
              <ul class="space-y-3">
                <li class="flex items-start">
                  <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                  <span class="text-gray-600">Digital insurance card</span>
                </li>
                <li class="flex items-start">
                  <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                  <span class="text-gray-600">Submit claims with photo</span>
                </li>
                <li class="flex items-start">
                  <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                  <span class="text-gray-600">Track reimbursements</span>
                </li>
                <li class="flex items-start">
                  <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                  <span class="text-gray-600">Access policy documents</span>
                </li>
              </ul>
            </div>
            <div>
              <h3 class="text-xl font-semibold text-gray-900 mb-4">Online Portal Benefits</h3>
              <ul class="space-y-3">
                <li class="flex items-start">
                  <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                  <span class="text-gray-600">Manage coverage options</span>
                </li>
                <li class="flex items-start">
                  <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                  <span class="text-gray-600">View and pay bills</span>
                </li>
                <li class="flex items-start">
                  <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                  <span class="text-gray-600">Download statements</span>
                </li>
                <li class="flex items-start">
                  <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
                  </svg>
                  <span class="text-gray-600">Chat with support</span>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- For Expats Section -->
  <div id="expats" class="bg-gray-50 py-16">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="max-w-3xl mx-auto">
        <h2 class="text-3xl font-bold text-gray-900 mb-8">For Expats</h2>
        
        <!-- Expat Benefits -->
        <div class="bg-white rounded-lg shadow-md p-6 mb-8">
          <h3 class="text-xl font-semibold text-gray-900 mb-4">Why Expats Choose CSS</h3>
          <ul class="space-y-3">
            <li class="flex items-start">
              <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
              </svg>
              <div>
                <span class="font-medium text-gray-900">English-Speaking Support</span>
                <p class="text-sm text-gray-600">Full service in English through all channels</p>
              </div>
            </li>
            <li class="flex items-start">
              <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
              </svg>
              <div>
                <span class="font-medium text-gray-900">Digital-First Experience</span>
                <p class="text-sm text-gray-600">Manage everything online or through the app</p>
              </div>
            </li>
            <li class="flex items-start">
              <svg class="h-5 w-5 text-primary-500 mt-1 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7" />
              </svg>
              <div>
                <span class="font-medium text-gray-900">International Coverage Options</span>
                <p class="text-sm text-gray-600">Travel insurance and worldwide emergency coverage</p>
              </div>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>

  <!-- How to Apply -->
  <div id="apply" class="bg-white py-16">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="max-w-3xl mx-auto">
        <h2 class="text-3xl font-bold text-gray-900 mb-8">How to Apply</h2>
        <div class="space-y-6">
          <div class="flex items-start">
            <div class="flex-shrink-0">
              <div class="flex items-center justify-center h-8 w-8 rounded-full bg-primary-100 text-primary-600 font-semibold">
                1
              </div>
            </div>
            <div class="ml-4">
              <h4 class="text-lg font-medium text-gray-900">Compare Plans</h4>
              <p class="mt-1 text-gray-600">Review different insurance models and coverage options</p>
            </div>
          </div>
          <div class="flex items-start">
            <div class="flex-shrink-0">
              <div class="flex items-center justify-center h-8 w-8 rounded-full bg-primary-100 text-primary-600 font-semibold">
                2
              </div>
            </div>
            <div class="ml-4">
              <h4 class="text-lg font-medium text-gray-900">Choose Model</h4>
              <p class="mt-1 text-gray-600">Select insurance model (standard, GP, or Telmed)</p>
            </div>
          </div>
          <div class="flex items-start">
            <div class="flex-shrink-0">
              <div class="flex items-center justify-center h-8 w-8 rounded-full bg-primary-100 text-primary-600 font-semibold">
                3
              </div>
            </div>
            <div class="ml-4">
              <h4 class="text-lg font-medium text-gray-900">Submit Application</h4>
              <p class="mt-1 text-gray-600">Apply with chosen provider</p>
            </div>
          </div>
          <div class="flex items-start">
            <div class="flex-shrink-0">
              <div class="flex items-center justify-center h-8 w-8 rounded-full bg-primary-100 text-primary-600 font-semibold">
                4
              </div>
            </div>
            <div class="ml-4">
              <h4 class="text-lg font-medium text-gray-900">Set Up Payments</h4>
              <p class="mt-1 text-gray-600">Arrange premium payments</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- FAQ Section -->
  <div class="bg-white py-16">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <h2 class="text-3xl font-bold text-gray-900 mb-12 text-center">Frequently Asked Questions</h2>
      <div class="max-w-3xl mx-auto">
        <dl class="space-y-8">
          {faqs.map(faq => (
            <div class="bg-white rounded-lg shadow-md p-6">
              <dt class="text-lg font-semibold text-gray-900 mb-2">{faq.question}</dt>
              <dd class="text-gray-600">{faq.answer}</dd>
            </div>
          ))}
        </dl>
      </div>
    </div>
  </div>

  <!-- Sticky CTA -->
  <div class="fixed bottom-0 inset-x-0 pb-2 sm:pb-5 z-50">
    <div class="max-w-7xl mx-auto px-2 sm:px-6 lg:px-8">
