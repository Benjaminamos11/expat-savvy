      <div class="max-w-3xl mx-auto">
        <h2 class="text-xl font-semibold text-gray-900 mb-4">In This Guide:</h2>
        <div class="bg-white rounded-lg shadow-sm p-4 md:p-6">
          <ul class="space-y-2 grid grid-cols-1 md:grid-cols-2 gap-x-4 gap-y-2">
            {tableOfContents.map(item => (
              <li>
                <a href={`#${item.id}`} class="text-primary-600 hover:text-primary-800 flex items-center">
                  <svg class="mr-2 h-4 w-4 text-gray-400" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd" />
                  </svg>
                  {item.title}
                </a>
              </li>
