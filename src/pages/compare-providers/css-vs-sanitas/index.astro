    sanitas: "Moderate (Varies by canton, age, deductible)"
  },
  {
    feature: "Supplementary: Alternative Medicine",
    css: "Good coverage with various myCSS supplementary plans",
    sanitas: "Strong focus, extensive coverage with Classic & Family plans"
  },
  {
    feature: "Supplementary: Check-ups & Prevention",
    css: "Good, contributions for various preventative measures",
    sanitas: "Good, various contributions for check-ups and prevention"
  },
  {
    feature: "Supplementary: Intl. Private Hospital",
    css: "Available (myCSS Hospital Insurance options)",
    sanitas: "Available (Hospital Extra Liberty, Hospital Top Liberty)"
