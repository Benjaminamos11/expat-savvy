              <th scope="col" class="px-6 py-4 text-left text-sm font-semibold text-primary-600 uppercase tracking-wider">
                CSS
              </th>
              <th scope="col" class="px-6 py-4 text-left text-sm font-semibold text-gray-600 uppercase tracking-wider">
                Sanitas
              </th>
            </tr>
          </thead>
          <tbody class="divide-y divide-gray-200">
            {tableData.map((row, index) => (
              <tr class={index % 2 === 0 ? 'bg-white' : 'bg-gray-50'}>
                <td class="px-6 py-4 text-sm font-medium text-gray-900">{row.feature}</td>
                <td class="px-6 py-4 text-sm text-gray-700">{row.css}</td>
                <td class="px-6 py-4 text-sm text-gray-700">{row.sanitas}</td>
