  {
    feature: "Supplementary: Gym & Health Promotion",
    css: "Contributions via myCSS Health Account (up to CHF 500)",
    sanitas: "Contributions via various plans (e.g., Sanitas Active App rewards)"
  },
  {
    feature: "Handling Pre-existing Conditions (Suppl.)",
    css: "Health questionnaire; potential for exclusions or premium loading",
    sanitas: "Health questionnaire; potential for exclusions or premium loading. Unique Individual Premium Supplement option for some conditions."
  },
  {
    feature: "Expat-Friendliness: English Support",
    css: "Good; English website, some English documents, English-speaking support",
    sanitas: "Very good; English website, comprehensive English documents, good English support"
  },
  {
    feature: "Expat-Friendliness: Onboarding Support",
    css: "Good general support for new clients",
    sanitas: "Good resources and clear information for expats"
  },
  {
    feature: "Customer Satisfaction (e.g., Comparis)",
    css: "Generally good ratings (e.g., 5.1/6 on Comparis 2023)",
    sanitas: "Consistently good ratings (e.g., 5.2/6 on Comparis 2023)"
